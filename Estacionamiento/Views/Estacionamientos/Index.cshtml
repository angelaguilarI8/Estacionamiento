@model IEnumerable<Estacionamiento.Models.Estacionamientos>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nivel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Placas)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nivel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Placas)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EstacionamientosID }) |
            @Html.ActionLink("Details", "Details", new { id=item.EstacionamientosID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EstacionamientosID })
        </td>
    </tr>
}

</table>
